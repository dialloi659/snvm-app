{

  "AppInfo": {
    "Version": "0.0.0",
    "Env": "DEV",
    "Comment": "Nothing"
  },


  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
